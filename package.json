{
  "name": "ximgdock",
  "displayName": "XImgDock",
  "description": "XImgDock brings instant live preview and one-click image insertion to VS Code, eliminating manual path input and streamlining XHTML editing for a faster, more intuitive workflow.",
  "version": "0.0.4",
  "publisher": "118-Oganesson",
  "icon": "./ximgdock_icon.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/118-Oganesson/ximgdock.git"
  },
  "bugs": {
    "url": "https://github.com/118-Oganesson/ximgdock/issues"
  },
  "homepage": "https://github.com/118-Oganesson/ximgdock#readme",
  "license": "MIT",
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "keywords": [
    "html",
    "xhtml",
    "preview",
    "live preview",
    "image",
    "wysiwyg"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ximgdock.showPreview",
        "title": "XImgDock: Open Preview",
        "icon": "$(open-preview)"
      },
      {
        "command": "ximgdock.showImageGallery",
        "title": "XImgDock: Show Image Gallery",
        "icon": "$(device-camera)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == xhtml || resourceLangId == html",
          "command": "ximgdock.showPreview",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == xhtml || resourceLangId == html",
          "command": "ximgdock.showImageGallery",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "24.3.0",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "9.34.0",
    "ts-loader": "9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  }
}